{
  "name": "nowlive",
  "displayName": "Now Live",
  "version": "1.2.1",
  "private": true,
  "description": "Instantly check which of your favorite Twitch streamers are live right from your favorite browser",
  "repository": "https://github.com/jewlexx/NowLive",
  "license": "MIT",
  "author": "Juliette Cordor",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "bun run build:wasm && bun run clean && rspack build --mode production",
    "build:wasm": "cd packages/image-helpers && bun run build",
    "cargo": "cd image-helpers && cargo",
    "clean": "rimraf dist",
    "dev": "rspack build --mode development",
    "fmt": "prettier --write ./src/*/**.{ts,tsx} --ignore-path .gitignore",
    "fmt:check": "prettier --check ./src/*/**.{ts,tsx} --ignore-path .gitignore",
    "icons": "bun scripts/icons.ts",
    "lint": "eslint ./src/*/**.{ts,tsx}",
    "lint:fix": "eslint ./src/*/**.{ts,tsx} --fix",
    "propegate:version": "node scripts/update-version.js",
    "start:firefox": "web-ext run --source-dir ./dist/"
  },
  "browserslist": "last 2 versions",
  "dependencies": {
    "@fontsource/inter": "^5.1.1",
    "@formkit/auto-animate": "^0.8.2",
    "image-helpers": "workspace:*",
    "motion": "12.4.7",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-icons": "5.5.0",
    "theme-change": "^2.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/plugin-syntax-jsx": "^7.25.9",
    "@babel/plugin-syntax-typescript": "^7.25.9",
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.21.0",
    "@rspack/cli": "^1.2.5",
    "@rspack/core": "^1.2.5",
    "@rspack/plugin-react-refresh": "^1.0.1",
    "@tailwindcss/postcss": "^4.0.8",
    "@tailwindcss/typography": "^0.5.16",
    "@types/dotenv-webpack": "^7.0.8",
    "@types/firefox-webext-browser": "120.0.4",
    "@types/node": "22.13.5",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.2.1",
    "babel-plugin-react-compiler": "^19.0.0-beta-e1e972c-20250221",
    "browserslist": "4.24.4",
    "cross-env": "7.0.3",
    "daisyui": "beta",
    "dotenv-webpack": "^8.1.0",
    "eslint": "9.21.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-compiler": "^19.0.0-beta-e1e972c-20250221",
    "eslint-plugin-react-hooks": "5.1.0",
    "postcss": "^8.5.3",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^10.1.5",
    "prettier": "^3.5.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "process": "0.11.10",
    "react-refresh": "^0.16.0",
    "rimraf": "6.0.1",
    "rspack-manifest-plugin": "^5.0.3",
    "sharp": "0.33.5",
    "tailwindcss": "^4.0.8",
    "ts-node": "^10.9.2",
    "typescript": "5.7.3",
    "typescript-eslint": "^8.25.0",
    "wasm-pack": "0.13.1",
    "web-ext": "8.4.0",
    "webpack-merge": "^6.0.1"
  },
  "trustedDependencies": [
    "@swc/core",
    "core-js",
    "image-helpers",
    "install-peers",
    "lmdb",
    "msgpackr-extract",
    "sharp",
    "spawn-sync",
    "wasm-pack"
  ]
}